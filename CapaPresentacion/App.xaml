             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CapaPresentacion.Principal"
             StartupUri="Principal/LoginUsuarios.xaml"
    xmlns:theme="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero2">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Estilos/Botones.xaml"/>
                <ResourceDictionary Source="/Estilos/BotonEliminar.xaml"/>
                <ResourceDictionary Source="/Estilos/ComboBox.xaml"/>
                <ResourceDictionary Source="/Estilos/DataGrid.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
